{
  "C_Cpp.default.includePath": ["/usr/local/include/mumps"],
  "cmake.configureOnOpen": false,
  "cmake.debugConfig": {
    "stopAtEntry": false,
    "MIMode": "gdb",
    "miDebuggerPath": "/usr/bin/gdb",
    "logging": {
      "trace": true,
      "engineLogging": true,
      "traceResponse": true
    }
  },
  "files.exclude": {
    "**/build/": true
  },
  "files.associations": {
    "*.C": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "*.tcc": "cpp",
    "cmath": "cpp",
    "utility": "cpp",
    "memory": "cpp",
    "numeric": "cpp"
  }
}
